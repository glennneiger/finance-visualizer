{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "A light foundation for your next frontend project based on webpack.",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "react-test-renderer": "^16.6.3",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "wait-for-expect": "^1.1.0",
    "webpack": "^4.26.1",
    "webpack-babel-jest": "^1.0.4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-jest": "^23.6.0",
    "bootstrap": "^4.1.3",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "nodemon": "^1.18.7",
    "popper.js": "^1.14.5",
    "react": "^16.6.3",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.6.3",
    "react-icons": "^3.2.2",
    "react-svg-piechart": "^2.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
